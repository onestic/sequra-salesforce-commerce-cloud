                <display-name xml:lang="x-default">Sequra Merchant ID Spain</display-name>
                <type>string</type>
                <mandatory-flag>false</mandatory-flag>
                <externally-managed-flag>false</externally-managed-flag>
                <min-length>0</min-length>
            </attribute-definition>
            <attribute-definition attribute-id="sequra_merchantID_IT">
                <display-name xml:lang="x-default">Sequra Merchant ID Italy</display-name>
                <type>string</type>
                <mandatory-flag>false</mandatory-flag>
                <externally-managed-flag>false</externally-managed-flag>
                <min-length>0</min-length>
            </attribute-definition>
            <attribute-definition attribute-id="sequra_merchantID_PT">
                <display-name xml:lang="x-default">Sequra Merchant ID Portugal</display-name>
                <type>string</type>
                <mandatory-flag>false</mandatory-flag>
                <externally-managed-flag>false</externally-managed-flag>
                <min-length>0</min-length>
            </attribute-definition>
            <attribute-definition attribute-id="sequraPaymentsDisplay">
				<display-name xml:lang="x-default">Display Sequra Payments in Checkout.</display-name>
				<description xml:lang="x-default">Control How Sequra Payments Methods are displayed in the checkout. Values: 1) Grouped: One Payment Method Sequra and when you click on it then the Sequra payment methods allowed (i1, sp1, pp3, pp5) are displayed 2) Separated: The Sequra Payments Methods allowed are displayed directly in the Checkout payments.</description>
				<type>enum-of-string</type>
				<mandatory-flag>false</mandatory-flag>
				<externally-managed-flag>false</externally-managed-flag>
				<value-definitions>
					<value-definition default="true">
						<display xml:lang="x-default">Grouped</display>
						<value>Grouped</value>
					</value-definition>
					<value-definition>
						<display xml:lang="x-default">Separated</display>
						<value>Separated</value>
					</value-definition>
				</value-definitions>
		    </attribute-definition>
	        </custom-attribute-definitions>
        <group-definitions>
