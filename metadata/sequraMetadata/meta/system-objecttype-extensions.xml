                <attribute attribute-id="sequra_merchantID_PT"/>
                <attribute attribute-id="sequra_merchantID_IT"/>
                <attribute attribute-id="sequra_assetKey"/>            
                <attribute attribute-id="sequra_Configuration"/>
                <attribute attribute-id="sequraPaymentsDisplay"/>
